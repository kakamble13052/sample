<table>
  <tr>
    <th>User Name</th>
    <th>First name</th>
    <th>Last name</th>
    <th colspan="3"></th>
  </tr>
  <% if current_user.present? %>
      <% @manager.users.all.each do |user| %>
          <tr>
            <td><%= user.username %></td>
            <td><%= user.firstname %></td>
            <td><%= user.lastname %></td>
          </tr>
      <% end %>
  <% end %>
</table>